{
  "manifest_version": 2,

  "name": "Next",
  "description": "Click 'Play Next' on classroom.udacity.com",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["https://classroom.udacity.com/*"],
      "js": ["next.js"]
    }
  ]

}

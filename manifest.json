{
  "manifest_version": 2,

  "name": "Next",
  "description": "Click 'Play Next' on classroom.udacity.com",
  "version": "1.2",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": [
          "https://classroom.udacity.com/*",
          "https://www.udemy.com/*"
      ],
      "js": ["next.js"]
    }
  ]

}
